{
  "service":{
    "path":"internal{{fileSeparator}}app{{fileSeparator}}{{`{{toSnakeCase .ServiceName}}`}}{{fileSeparator}}service",
    "file_name":"service.go",
    "interface_name":"{{`{{toUpperFirstCamelCase .ServiceName}}`}}Service",
    "struct_name":"stub{{`{{toCamelCase .ServiceName}}`}}Service"
  },
  "middleware":{
    "name":"middleware.go"
  },
  "endpoints":{
    "path":"internal{{fileSeparator}}app{{fileSeparator}}{{`{{toSnakeCase .ServiceName}}`}}{{fileSeparator}}endpoints",
    "file_name":"endpoints.go",
    "requests_file_name":"requests.go",
    "responses_file_name":"responses.go"
  },
  "transports":{
    "path":"internal{{fileSeparator}}app{{fileSeparator}}{{`{{toSnakeCase .ServiceName}}`}}{{fileSeparator}}transports",
    "file_name":"{{`{{.TransportType}}`}}.go"
  },
  "pb":{
    "path":"pb{{fileSeparator}}{{`{{toSnakeCase .ServiceName}}`}}"
  },
  "cmd":{
   "path":"cmd{{fileSeparator}}{{`{{toSnakeCase .ServiceName}}`}}",
   "file_name":"main.go"
  },
  "custom_errors": {
    "path": "internal{{fileSeparator}}pkg{{fileSeparator}}errors",
    "file_name": "errors.go"
  },
  "custom_responses": {
    "path": "internal{{fileSeparator}}pkg{{fileSeparator}}responses",
    "responses_file_name": "responses.go",
    "errors_file_name": "errors.go"
  },
  "default_transport":"http"
}